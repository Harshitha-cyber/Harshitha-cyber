class shape():
  def area(self):
    pass
class rectangle(shape):
  def __init__(self,length,width):
    self.length=length
    self.width=width
  def area(self):
    return self.length*self.width
class circle(shape):
  def __init__(self,radius):
    self.radius=radius
  def area(self):
    return self.radius*3.14*self.radius
shapes= [rectangle(4,5),circle(4)]
for i in shapes:
    print(f"the area of the shape is{i.area()}")
